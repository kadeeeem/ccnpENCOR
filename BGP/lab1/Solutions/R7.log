hostname R7
!
interface Loopback0
 ip address 166.6.0.1 255.255.255.0
!
interface Loopback1
 ip address 166.6.1.1 255.255.255.0
!
interface Loopback2
 ip address 166.6.2.1 255.255.255.0
!
interface Loopback1234
 ip address 1.2.3.7 255.255.255.255
!
interface GigabitEthernet0/0
 description LINK TO R5
 ip address 3.6.3.7 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description LINK TO R6
 ip address 7.6.7.7 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/2
 description LINK TO R8
 ip address 2.6.2.7 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
router bgp 300
 bgp log-neighbor-changes
 no bgp default ipv4-unicast
 neighbor 2.6.2.8 remote-as 400
 neighbor 3.6.3.5 remote-as 200
 neighbor 7.6.7.6 remote-as 200
 !
 address-family ipv4
  network 1.2.3.7 mask 255.255.255.255
  network 2.6.2.0 mask 255.255.255.0
  network 3.6.3.0 mask 255.255.255.0
  network 7.6.7.0 mask 255.255.255.0
  network 166.6.0.0 mask 255.255.255.0
  network 166.6.1.0 mask 255.255.255.0
  network 166.6.2.0 mask 255.255.255.0
  neighbor 2.6.2.8 activate
  neighbor 3.6.3.5 activate
  neighbor 3.6.3.5 route-map PREPEND out
  neighbor 7.6.7.6 activate
 exit-address-family
!
ip access-list standard LOCAL-IPs
 permit 166.6.0.0 0.0.255.255
!
route-map PREPEND permit 10
 match ip address LOCAL-IPs
 set as-path prepend 800 900 1000
