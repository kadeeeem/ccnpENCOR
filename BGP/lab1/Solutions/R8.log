hostname R8
!
interface Loopback0
 ip address 8.8.8.33 255.255.255.224
!
interface Loopback1
 ip address 8.8.8.65 255.255.255.240
!
interface Loopback2
 ip address 8.8.8.81 255.255.255.248
!
interface Loopback1234
 ip address 1.2.3.8 255.255.255.255
!
interface GigabitEthernet0/0
 description LINK TO R7
 ip address 2.6.2.8 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description LINK TO R6
 ip address 2.7.2.8 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/2
 description LINK TO R9
 ip address 2.5.2.8 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
router bgp 400
 bgp log-neighbor-changes
 no bgp default ipv4-unicast
 neighbor 2.5.2.9 remote-as 100
 neighbor 2.6.2.7 remote-as 300
 neighbor 2.7.2.6 remote-as 200
 !
 address-family ipv4
  network 1.2.3.8 mask 255.255.255.255
  network 2.5.2.0 mask 255.255.255.0
  network 2.6.2.0 mask 255.255.255.0
  network 2.7.2.0 mask 255.255.255.0
  network 8.8.8.32 mask 255.255.255.224
  network 8.8.8.64 mask 255.255.255.240
  network 8.8.8.80 mask 255.255.255.248
  neighbor 2.5.2.9 activate
  neighbor 2.6.2.7 activate
  neighbor 2.7.2.6 activate
  neighbor 2.7.2.6 route-map PREPEND out
 exit-address-family
!
ip access-list standard LOOPBACK-IPs
 permit 8.8.8.0 0.0.0.255
!
route-map PREPEND permit 10
 match ip address LOOPBACK-IPs
 set as-path prepend 456 7889 123
