hostname R9
!
interface Loopback0
 ip address 5.5.5.1 255.255.255.0
!
interface Loopback1
 ip address 5.5.55.1 255.255.255.0
!
interface Loopback1234
 ip address 1.2.3.9 255.255.255.255
!
interface GigabitEthernet0/0
 description LINK TO R1
 ip address 1.5.1.9 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description LINK TO R8
 ip address 2.5.2.9 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
router bgp 100
 bgp log-neighbor-changes
 no bgp default ipv4-unicast
 neighbor 1.5.1.1 remote-as 200
 neighbor 2.5.2.8 remote-as 400
 !
 address-family ipv4
  network 1.2.3.9 mask 255.255.255.255
  network 1.5.1.0 mask 255.255.255.0
  network 2.5.2.0 mask 255.255.255.0
  network 5.5.5.0 mask 255.255.255.0
  network 5.5.55.0 mask 255.255.255.0
  neighbor 1.5.1.1 activate
  neighbor 2.5.2.8 activate
 exit-address-family
!
