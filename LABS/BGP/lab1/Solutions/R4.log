hostname R4
!
interface Loopback0
 ip address 200.2.0.1 255.255.255.0
!
interface Loopback1
 ip address 200.2.1.1 255.255.255.128
!
interface Loopback2
 ip address 200.2.2.1 255.255.255.192
!
interface Loopback1234
 ip address 1.2.3.4 255.255.255.255
 ip ospf 1 area 0
!
interface GigabitEthernet0/0
 description LINK TO R2
 ip address 1.2.1.4 255.255.255.0
 ip ospf 1 area 0
 duplex auto
 speed auto
 media-type rj45
!
router ospf 1
 router-id 10.0.0.4
!
router bgp 200
 bgp log-neighbor-changes
 neighbor 1.2.3.1 remote-as 200
 neighbor 1.2.3.1 update-source Loopback1234
 neighbor 1.2.3.2 remote-as 200
 neighbor 1.2.3.2 update-source Loopback1234
 neighbor 1.2.3.3 remote-as 200
 neighbor 1.2.3.3 update-source Loopback1234
 neighbor 1.2.3.5 remote-as 200
 neighbor 1.2.3.5 update-source Loopback1234
 neighbor 1.2.3.6 remote-as 200
 neighbor 1.2.3.6 update-source Loopback1234
 !
 address-family ipv4
  network 200.2.0.0
  network 200.2.1.0 mask 255.255.255.128
  network 200.2.2.0 mask 255.255.255.192
  neighbor 1.2.3.1 activate
  neighbor 1.2.3.2 activate
  neighbor 1.2.3.3 activate
  neighbor 1.2.3.5 activate
  neighbor 1.2.3.6 activate
 exit-address-family
!
