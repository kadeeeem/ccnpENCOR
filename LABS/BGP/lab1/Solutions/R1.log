hostname R1
!
interface Loopback1234
 ip address 1.2.3.1 255.255.255.255
 ip ospf 1 area 0
!
interface GigabitEthernet0/0
 description LINK TO R2
 ip address 1.1.1.1 255.255.255.0
 ip ospf 1 area 0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description LINK TO R9
 ip address 1.5.1.1 255.255.255.0
 ip ospf 1 area 0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/2
 description LINK TO R3
 ip address 1.4.1.1 255.255.255.0
 ip ospf 1 area 0
 duplex auto
 speed auto
 media-type rj45
!
router ospf 1
 router-id 10.0.0.1
 passive-interface GigabitEthernet0/1
!
router bgp 200
 bgp log-neighbor-changes
 no bgp default ipv4-unicast
 neighbor 1.2.3.2 remote-as 200
 neighbor 1.2.3.2 update-source Loopback1234
 neighbor 1.2.3.3 remote-as 200
 neighbor 1.2.3.3 update-source Loopback1234
 neighbor 1.2.3.4 remote-as 200
 neighbor 1.2.3.4 update-source Loopback1234
 neighbor 1.2.3.5 remote-as 200
 neighbor 1.2.3.5 update-source Loopback1234
 neighbor 1.2.3.6 remote-as 200
 neighbor 1.2.3.6 update-source Loopback1234
 neighbor 1.5.1.9 remote-as 100
 !
 address-family ipv4
  aggregate-address 8.8.8.0 255.255.255.0 as-set summary-only
  neighbor 1.2.3.2 activate
  neighbor 1.2.3.3 activate
  neighbor 1.2.3.4 activate
  neighbor 1.2.3.5 activate
  neighbor 1.2.3.6 activate
  neighbor 1.5.1.9 activate
 exit-address-family

