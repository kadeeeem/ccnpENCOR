hostname R4
!
interface Loopback0
 ip address 4.4.4.4 255.255.255.255
 ip ospf hello-interval 1
!
interface GigabitEthernet0/0
 ip address 172.16.10.212 255.255.255.248
 ip ospf hello-interval 1
 duplex auto
 speed auto
 media-type rj45
!
router ospf 1
 network 0.0.0.0 255.255.255.255 area 0
!
